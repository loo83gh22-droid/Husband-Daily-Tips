{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/cron/send-tomorrow-tips",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/send-7-day-survey-email",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/send-30-and-90-day-survey-emails",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/check-expiring-trials",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/downgrade-expired-trials",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/update-seasonal-dates",
      "schedule": "0 0 1 1 *"
    }
  ]
}


